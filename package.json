{
    "name": "prop2yaml",
    "displayName": "Properties To Yaml",
    "description": "Convert properties to yaml",
    "version": "0.0.6",
    "publisher": "tanaka-x",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:prop2yaml.convert"
    ],
    "main": "./out/prop2yaml",
    "contributes": {
        "menus": {
            "editor/context": [
                {
                    "command": "prop2yaml.convert",
                    "group": "9_cutcopypaste@500"
                }
            ]
        },
        "commands": [
            {
                "command": "prop2yaml.convert",
                "title": "Convert properties to yaml"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tanaka-x/prop2yaml.git"
    }
}
